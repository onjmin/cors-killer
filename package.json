{
	"author": "onjmin",
	"license": "AGPL-3.0",
	"name": "@onjmin/cors-killer",
	"version": "1.0.11",
	"description": "CORS制限を回避するモジュール",
	"homepage": "https://onjmin.github.io/cors-killer",
	"repository": {
		"type": "git",
		"url": "https://github.com/onjmin/cors-killer.git"
	},
	"bugs": {
		"url": "https://github.com/onjmin/cors-killer/issues"
	},
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"scripts": {
		"dev": "rimraf dist && tsup && biome format src --write && tsx demo/server.ts",
		"build": "rimraf dist && tsup && biome check src && biome format src --write",
		"test": "vitest",
		"test:coverage": "vitest run --coverage",
		"prepublishOnly": "pnpm run build && pnpm run test",
		"patch": "pnpm version patch",
		"publish-dangerous-dont-use-this-command": "pnpm publish --access public"
	},
	"files": [
		"dist"
	],
	"keywords": [
		"CORS",
		"cors-bypass",
		"cors-proxy",
		"cross-origin",
		"url-sanitizer",
		"safe-url",
		"canvas",
		"fetch",
		"image",
		"proxy"
	],
	"volta": {
		"node": "22.19.0",
		"pnpm": "10.15.0"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.2",
		"@hono/node-server": "1.19.5",
		"@vitest/coverage-v8": "3.2.4",
		"hono": "4.10.3",
		"rimraf": "6.0.1",
		"tsup": "8.5.0",
		"tsx": "4.20.6",
		"typedoc": "0.28.11",
		"typescript": "5.9.2",
		"vitest": "3.2.4"
	}
}
